<div class="preloader"><div class="status">&nbsp;</div></div>

<header id="home" class="header">

  <div id="main-nav" class="navbar navbar-inverse bs-docs-nav" role="banner">

    <div class="container">

      <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>

      <div class="navbar-header responsive-logo">
        <a data-scroll href="#" class="navbar-brand"><img src="images/logo.png" alt="Theme Preview"></a>
      </div>

    </div>

  </div> <!-- / END MAIN NAV -->

  <!-- / END TOP BAR -->
  <div class="home-header-wrap">

    <div class="header-content-wrap">
      <div class="container">
        <h1 class="intro-text">{{index.thanks.message}}</h1>
        <div class="cta">
          <a href="{{index.thanks.button.link}}" class="btn btn-primary custom-button red-btn">{{index.thanks.button.text}}</a>
        </div>
      </div>
    </div>
    <!-- .header-content-wrap -->
    <div class="clear"></div>
  </div>
</header> <!-- / END HOME SECTION  -->

<footer id="footer" role="contentinfo">


<div class="container">

  <div class="col-md-6 company-details endeva">
    <h4><img src="images/endeva.png" alt="Endeva logo">{{index.footer.endeva}}</h4>
    <p>{{{index.footer.endevaDescription}}}</p>
  </div>

  {{#each index.footer.contacts}}

  <div class="col-md-3 company-details contact-info">
    <h4 style="margin-top: 28px">{{country}}</h4>
    <p><i class="fa fa-user" aria-hidden="true"></i> {{contact}}</p>
    <p><i class="fa fa-envelope" aria-hidden="true"></i> {{email}}</p>
    <p><i class="fa fa-phone" aria-hidden="true"></i> {{phone}}</p>
  </div>

  {{/each}}

</div>

</div> <!-- / END CONTAINER -->

</footer> <!-- / END FOOOTER  -->