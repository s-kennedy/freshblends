<div class="preloader"><div class="status">&nbsp;</div></div>

<header id="home" class="header">

  <div id="main-nav" class="navbar navbar-inverse bs-docs-nav" role="banner">

    <div class="container">

      <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>

      <div class="navbar-header responsive-logo">
        <a data-scroll href="{{root}}" class="navbar-brand"><img src="images/logo.png" alt="Theme Preview"></a>
      </div>

      <nav class="navbar-collapse bs-navbar-collapse collapse" role="navigation"   id="site-navigation">
        <ul class="nav navbar-nav navbar-right responsive-nav main-nav-list">
          {{#each index.navigation }}
          <li class="page_item"><a data-scroll href="#{{id}}">{{title}}</a></li>
          {{/each}}
        </ul>
      </nav>

    </div>

  </div> <!-- / END MAIN NAV -->

  <!-- / END TOP BAR -->
  <div class="home-header-wrap">

    <div class="header-content-wrap">
      <div class="container">
        <h1 class="intro-text">{{index.header.intro}}</h1>
        <div class="cta">
          <a data-scroll href="#about"><i class="fa fa-chevron-down" aria-hidden="true"></i></a>
        </div>
      </div>
    </div>
    <!-- .header-content-wrap -->
    <div class="clear"></div>
  </div>
</header> <!-- / END HOME SECTION  -->

<div id="content" class="site-content">

<section class="focus" id="about">

<div class="container">

  <!-- SECTION HEADER -->

  <div class="section-header">

    <!-- SECTION TITLE -->

    <h2 class="dark-text">{{index.navigation.about.title}}</h2>
    <div class="section-legend">{{index.about.legend}}</div>
  </div>

  <div class="row">
    {{#each index.about.columns}}
    <div class="col-lg-3 col-sm-3 focus-box" data-scrollreveal="enter left after 0.15s over 1s">
      <div class="service-icon">
        <i class="pixeden" style="background:url(images/{{image}}) no-repeat center;width:100%; height:100%;"></i> <!-- FOCUS ICON-->
      </div>
          
      <h3 class="red-border-bottom subtitle">{{title}}</h3>
      <!-- FOCUS HEADING -->
      <p class="emphasis dark-text">{{{subtitle}}}</p>
      <p>{{{text}}}</p>

    </div>
    {{/each}}
    <div class="updates"><a href="concept_note.pdf" target="_blank" class="btn btn-primary custom-button red-btn">Download the concept note</a></div>

</div> <!-- / END CONTAINER -->

</section>  <!-- / END FOCUS SECTION -->

<section class="about-us" id="offer">
  <div class="container">

    <!-- SECTION HEADER -->

    <div class="section-header">

      <!-- SECTION TITLE -->

      <h2 class="white-text">{{index.navigation.offer.title}}</h2>
      <!-- SHORT DESCRIPTION ABOUT THE SECTION -->

      <div class="white-text section-legend">{{index.offer.legend}}
      </div>    
    </div><!-- / END SECTION HEADER -->

    <!-- 3 COLUMNS OF ABOUT US-->

    <div class="row">

      <!-- COLUMN 1 - BIG MESSAGE ABOUT THE COMPANY-->

    <div class="col-lg-4 col-md-4 column zerif-rtl-big-title">
      <div class="big-intro" data-scrollreveal="enter left after 0s over 1s">
        {{index.offer.bigText}}
      </div>
      {{!-- <p style="text-align:right;">{{index.offer.smallText}}</p> --}}
    </div>

    <div class="col-lg-4 col-md-4 column zerif-rtl-skills ">

      <ul class="skills" data-scrollreveal="enter right after 0s over 1s">

        <!-- SKILLS -->
        {{#each index.offer.services.column1.skills}}
        <li class="skill">
          <div class="skill-count"><i class="fa fa-check" aria-hidden="true"></i></div>
          <div class="section-legend">{{skill}}</div>
        </li>
        {{/each}}
      </ul>

    </div> <!-- / END SKILLS COLUMN 1-->

      <div class="col-lg-4 col-md-4 column zerif-rtl-skills ">
        <ul class="skills" data-scrollreveal="enter right after 0s over 1s">
        {{#each index.offer.services.column2.skills}}        
        <li class="skill skill_5">
          <div class="skill-count"><i class="fa fa-check" aria-hidden="true"></i></div>
          <div class="section-legend">{{skill}}</div>
        </li>
        {{/each}}
      </ul>

    </div> <!-- / END SKILLS COLUMN 2-->

    
  </div> <!-- / END 3 COLUMNS OF ABOUT US-->

  <!-- CLIENTS -->
  
</div> <!-- / END CONTAINER -->

  <div class="container">
    <div class="row" data-scrollreveal="enter right after 0s over 1s">
      <div class="col-md-12">
        <div id="client-feedbacks" class="owl-carousel owl-theme  ">

          {{#each index.offer.packages}}
          <div class="feedback-box">

              <h3 class="red-border-bottom subtitle">{{title}}</h3>
              <div class="message">
              <h4>{{{description}}}</h4>
                <ul>
                  {{#each features}}
                  <li>{{item}}</li>
                  {{/each}}
                </ul>
              </div>

          </div>
          {{/each}}

        </div>
      </div>
    </div>
  </div>
</section> <!-- END ABOUT US SECTION -->

{{!-- <section class="testimonial" id="solutions">
</section>  --}}

<section class="our-team" id="team">
  <div class="container">
    <div class="section-header">
      <h2 class="dark-text">{{ index.navigation.team.title }}</h2>

      <div class="section-legend">{{ index.team.legend }}</div>
    </div>

    <div class="row" data-scrollreveal="enter left after 0s over 0.5s">

      {{#each index.team.members }}
      <div class="col-lg-3 col-sm-3 team-box">

        <div class="team-member">
          <figure class="profile-pic">
            <img src="images/{{image}}" alt="Uploaded image" />
          </figure>
        
          <div class="member-details">
            <h3 class="dark-text red-border-bottom">{{name}}</h3>
            <div class="position">{{position}}</div>
          </div>

          <div class="details">
            {{details}}
          </div>
        
        </div>
      </div>
      {{/each}}

    </div>
  </div>
</section> 

<section class="our-board" id="board">
  <div class="container">
    <div class="section-header">
      <h2 class="white-text">{{ index.navigation.board.title }}</h2>

      <div class="section-legend white-text">{{ index.board.legend }}</div>
    </div>

    <div class="row" data-scrollreveal="enter right after 0s over 0.5s">

      {{#each index.board.members }}
      <div class="col-lg-3 col-sm-3 team-box">

        <div class="team-member">
          <figure class="profile-pic">
            <img src="images/{{image}}" alt="Uploaded image" />
          </figure>
        
          <div class="member-details">
            <h3 class="white-text red-border-bottom">{{name}}</h3>
            <div class="position">{{position}}</div>
          </div>

          <div class="details">
            {{details}}
          </div>
        
        </div>
      </div>
      {{/each}}

    </div>
  </div>
</section> 

<section class="latest-news" id="news">
  <div class="container">
    <div class="section-header">
      <h2 class="white-text">{{ index.navigation.news.title}}</h2>
      <div class="white-text section-legend">{{index.news.legend}}</div>
      <div class="updates"><a href="//www.mailchimp.com/" target="_blank" class="btn btn-primary custom-button green-btn">Sign up to receive updates</a></div>
    </div><!-- END .section-header -->

  <div class="clear"></div>

  <div id="carousel-homepage-latestnews" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner" role="listbox">
      <div class="item active">
        {{#each index.news.articles}}
        <div class="latestnews-box {{class}}">
          <div class="latestnews-img" style="background:url({{image}}) no-repeat center;">
          </div>

          <div class="latesnews-content">
            <h3 class="latestnews-title"><a href="{{link}}" target="_blank" title="{{title}}">{{title}}</a></h3>
            <p class="white-text"><span>{{date}}</span> - {{text}}</p>
            <p><a href="{{link}}" target="_blank">Read more</a></p>
          </div>
        </div><!-- .latestnews-box"> -->
        {{/each}}

        <div class="clear"></div>
      </div>
    </div><!-- .carousel-inner -->

    <a class="left carousel-control" href="#carousel-homepage-latestnews" role="button" data-slide="prev">
      <i class="fa fa-chevron-left" aria-hidden="true"></i><span class="sr-only">Previous</span>
    </a>

    <a class="right carousel-control" href="#carousel-homepage-latestnews" role="button" data-slide="next">
      <i class="fa fa-chevron-right" aria-hidden="true"></i></span><span class="sr-only">Next</span>
    </a>
  </div><!-- #carousel-homepage-latestnews -->
</div><!-- .container -->
</section>



